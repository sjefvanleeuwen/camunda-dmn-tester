    "version": "0.2.0",
    "configurations": [
        {
            "name": "Console, Create Test Table",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/dmn-test/bin/Debug/netcoreapp2.2/dmn-test.dll",
            "args": ["-e","http://localhost:8080/engine-rest",
                     "-m","create-test-table.md",
                     "-o","create",
                     "-k","invoiceClassification"],
            "cwd": "${workspaceFolder}/src/dmn-test",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}